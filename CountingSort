class Result {
    public static List<Integer> countingSort(List<Integer> arr) {
    ArrayList arr2=new ArrayList();
    for(int i=0;i<arr.size();i++){
        int c=0;
        for(int j=0;j<arr.size();j++){
           if(i==arr.get(j))
            c++; 
        }
        arr2.add(c);
        
    }
    return arr2;
    }

}
